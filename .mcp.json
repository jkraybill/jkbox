{
  "mcpServers": {
    "gordo-memory": {
      "command": "/usr/bin/node",
      "args": [
        "/home/jk/gordo-framework/mcp-servers/gordo-memory/dist/index.js"
      ],
      "env": {
        "NODE_PATH": "/home/jk/gordo-framework/mcp-servers/gordo-memory/node_modules",
        "REPO_PATH": "/home/jk/jkbox",
        "OLLAMA_URL": "http://localhost:11434",
        "EMBEDDING_MODEL": "mxbai-embed-large:latest"
      }
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-git",
        "--repository",
        "/home/jk/jkbox"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    }
  }
}
